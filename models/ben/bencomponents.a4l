MODEL component_constants(
	supported_components IS_A set OF symbol_constant;
) REFINES compmodel;

	rpp_index		IS_A 	integer_constant; (* reid prausnitz and poling # *)
	formula			IS_A	symbol_constant;
	subgroups		IS_A	set OF symbol_constant;
	groups			IS_A	set OF integer_constant;

	wilson_set		IS_A	set OF symbol_constant;
	uc			IS_A	UNIFAC_constants;
	mw			IS_A	molar_weight_constant;
	Tb			IS_A	boiling_temperature;
	Tc			IS_A	critical_temperature;
	Pc			IS_A	critical_pressure;
	Vc			IS_A	critical_volume;
	Zc			IS_A	critical_compressibility;
	omega			IS_A	acentric_factor;
	cpvapa			IS_A	heat_capacity_a_constant;
	cpvapb			IS_A	heat_capacity_b_constant;
	cpvapc			IS_A	heat_capacity_c_constant;
	cpvapd			IS_A	heat_capacity_d_constant;
	Hf			IS_A	enthalpy_of_formation_constant;
	Gf			IS_A	free_energy_of_formation_constant;

	vpa, vpb, vpc, vpd	IS_A	vapor_pressure_constant;
	vp_correlation		IS_A 	integer_constant;
	(* vpa, vpb, vpc, vpd are defined as wild real refinements.
	 * They must be assigned values with appropriate units.
	 * The values in RPP have units appropriate (but unstated)
	 * to the vp_correlation specified.
	 * For Equation 1: vpa, vpb, vpc, vpd are DIMENSIONLESS (no units).
	 * For Equation 2: vpa, vpc are DIMENSIONLESS, vpb is in {K}, 
	 *                 and vpd is {K^2/bar}.
	 * For Equation 3: vpa DIMENSIONLESS, vpb, vpc are in {K}, vpd not used.
	 *)

	Hv			IS_A	heat_of_vaporization_constant;
	Tliq			IS_A	reference_temperature;
	Vliq			IS_A	reference_molar_volume;
	lden			IS_A	reference_mass_density;
	T0			IS_A	reference_temperature;
	P0			IS_A	reference_pressure;
	H0, G0			IS_A	reference_molar_energy;
	S0			IS_A	reference_molar_entropy;

	T0 :== 298.15{K};
	P0 :== 1.0{atm};

(* data checked by: Jennifer L Perry 1/97 *)

END component_constants;

