MODEL z;

  HL		  IS_A heap_leach;
  HL.n_lado		:==	2;
  HL.n_largo	:==	2;
  

METHODS

    METHOD clear;
	RUN HL.clear;
    END clear;

    METHOD seqmod;
      RUN HL.seqmod;
    END seqmod;

    METHOD specify;
      RUN HL.specify;
    END specify;

    METHOD reset;
	RUN clear;
	RUN specify;
    END reset;

    METHOD scale;
    END scale;


END z;




(***********************************************************)

