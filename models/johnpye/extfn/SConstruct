import os
env = Environment(ENV=os.environ,tools=['mingw'])

srcs = Split("""
	extfntest.c
""")

env.SharedLibrary("extfntest",srcs
	, CPPPATH=['../../../base/generic']
	, LIBPREFIX=''
	, LIBS=['ascend']
	, LIBPATH=['../../..']
)

