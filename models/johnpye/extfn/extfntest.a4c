REQUIRE "system.a4l";
IMPORT "johnpye/extfn/extfntest";

MODEL test_extfntest;

	y IS_A solver_var;
	x IS_A solver_var;

	do_addone: add_one(
		x : INPUT; 
		y : OUTPUT
	);

METHODS

METHOD specify;
	FIX x;
END specify;
METHOD values;
	x := 1.0;
END values;

METHOD default_self;
	RUN specify; RUN values;
END default_self;

METHOD self_test;
	ASSERT y == 2;
END self_test;

END test_extfntest;
