(* test of the fluid properties routines currently implemented in asc_mbwr.c *)

REQUIRE "atoms.a4l";
IMPORT "johnpye/fprops/helmholtz";

MODEL mbwrconf;
	component IS_A symbol_constant;
	component :== 'R123';
END mbwrconf;

MODEL fprops_test;

	p IS_A pressure;
	T IS_A temperature;
	rhob IS_A molar_density;

	propsdata IS_A mbwrconf;

	props: mbwr_p(
		T, rhob : INPUT;
		p : OUTPUT;
		propsdata : DATA
	);

METHODS
METHOD on_load;
	FIX rhob, T;
	rhob := 1 {mol/m^3};
	T := 400 {K};
END on_load;
END fprops_test;

