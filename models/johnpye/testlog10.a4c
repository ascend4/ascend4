REQUIRE "atoms.a4l";

MODEL testlog10;

	x IS_A solver_var;
	y IS_A solver_var;
	z IS_A solver_var;

	log_10_expr: y = log10(x);
	log_e_expr: z = ln(x);

METHODS
METHOD specify;
	x.fixed := TRUE;
END specify;
METHOD values;
	x := 10 {1};
END values;
METHOD default_self;
	RUN reset;
	RUN values;
END default_self;
METHOD self_test;
	ASSERT abs(y - 1) < 1e-20;
	ASSERT abs(z - 2.302585093) < 1e-8;
END self_test;

END testlog10;
