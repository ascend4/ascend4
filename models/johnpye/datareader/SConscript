Import('env')
dr_env = env.Copy()

srcs = Split("""
	datareader.c
	dr.c
	tmy.c
	texttable.c
	sun.c
""")

dr_env.Append(LIBPATH=['#'])
dr_env.Append(LIBS=['ascend'])

lib = dr_env.SharedLibrary("datareader",srcs
	,SHLIBSUFFIX = env['EXTLIB_SUFFIX']
	,SHLIBPREFIX = env['EXTLIB_PREFIX']
)

env['extfns'] += [lib]

