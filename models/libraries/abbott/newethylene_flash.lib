      alpha['propane'] 		:= 0.500;
      alpha['propadiene'] 	:= 0.03124;
      alpha['propylene'] 	:= 0.01865;
      alpha['i_butane'] 	:= 0.05473;
      VLE.T := 233 {K};
      VLE.P := 100.0 {psia};
