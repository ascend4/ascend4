      liqsplit[liqouts].fixed			:= FALSE;
      vapsplit[vapouts].fixed			:= FALSE;
      reboil_ratio.fixed 			:= FALSE;
      prodsplit['bottoms'].fixed		:= FALSE;
      prodsplit['vapor_product'].fixed 	:= FALSE;
