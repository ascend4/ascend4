REQUIRE "iapwssat.a4c";

MODEL satsteamstream REFINES iapwssat;
	mdot IS_A mass_rate;
	Hdot IS_A energy_rate;
	Hdot = mdot*h;
	rho IS_A mass_density;
	rho * v = 1;
METHODS
METHOD specify;
	RUN iapwssat::specify;
	FIX mdot;
END specify;
METHOD values;
	RUN iapwssat::values;
	mdot := 0.1 {kg/s};
END values;
METHOD on_load;
	RUN bound_self;
	RUN reset;
	RUN values;
END on_load;
END satsteamstream;
