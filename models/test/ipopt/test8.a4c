MODEL test8;
	NOTES
	  'author' SELF {Mahesh Narayanamurthi}
	  'description' SELF {
             Adapted from : Hock-Schittkowsky test suite
             Problem Number: 66
             Solution: x* = [.1841,1.2021,3.3273] ; f(x*) = .58
	     Url: http://www.math.uni-bayreuth.de/~kschittkowski/tp_coll1.htm
	    }
	  'creation date' SELF {July, 2009}
	END NOTES;

	x1,x2,x3 IS_A factor;
	
	cons81: x2 - exp(x1)>=0;
	cons82: x3 - exp(x2)>=0;
	

	objective8: MAXIMIZE .2*x3 - .8*x1;
METHODS

METHOD self_test;
	ASSERT abs(x1 - .1841) < 1e-4;
	ASSERT abs(x2 - 1.2021) < 1e-4;
	ASSERT abs(x3 - 3.3273) < 1e-4;
END self_test;


METHOD bound_self;
	x1.lower_bound := 0;
	x2.lower_bound := 0;
	x3.lower_bound := 0;

	x1.upper_bound := 100;
	x2.upper_bound := 100;
	x3.upper_bound := 10;

END bound_self;

METHOD default_self;
	x1 := 0;
	x2 := 1.05;
	x3 := 2.9;
END default_self;

METHOD on_load;
        RUN bound_self;
        RUN default_self;
END on_load;

END test8;

