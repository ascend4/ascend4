# script for executing simpleFS.V1.a4c or simpleFS.V2.a4c

#execute the first three commands
DELETE TYPES;
DELETE SYSTEM;
DELETE tc;

# execute one of the following READ FILE commands
READ FILE "simpleFS.V1.a4c";
READ FILE "simpleFS.V2.a4c";

# execute the remaining commands
COMPILE tc OF test_controller;
BROWSE {tc};
RUN {tc.values};
RUN {tc.reset};
SOLVE {tc} WITH QRSlv;








