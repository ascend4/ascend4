(* requires an empty system, and preparameterized library *)
REQUIRE system;
REQUIRE atoms;
REQUIRE components;
REQUIRE H_G_thermodynamics;
REQUIRE stream;

MODEL noc;
  rc IS_A Rackett_component;
  rc.data IS_REFINED_TO n_octane;
  st IS_A vapor_stream;
  st.components :== ['noc'];
  st.data['noc'] IS_REFINED_TO n_octane;
END noc;
