echo "${ECHO_T}$LPAKLIB" >&6
else
  echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
fi

  test -n "$LPAKLIB" && break
done

        lpak_src_file="$srcdir/../../linpack/d1mach.f"
        if test -z "$LPAKLIB" -a -n "$lpak_src_file" ; then
