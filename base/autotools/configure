# Check whether --enable-dynamic-linking or --disable-dynamic-linking was given.
if test "${enable_dynamic_linking+set}" = set; then
  enableval="$enable_dynamic_linking"
  if test $enableval; then
		dynlinking=1
	else
		dynlinking=0
	fi

else
  dynlinking="_def"

fi;

